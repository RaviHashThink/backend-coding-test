{
  "name": "backend-coding-test",
  "version": "1.0.0",
  "description": "A coding assessment for Xendit backend interviews",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint ./src/",
    "test": "nyc mocha tests",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "start": "node index.js",
    "ts_start": "node dist/index.js",
    "ts_dev": "nodemon ts_src/index.ts",
    "ts_build": "tsc -p .",
    "artillery": "artillery run artillery_config.yml"
  },
  "pre-push": [
    "test"
  ],
  "keywords": [
    "Xendit"
  ],
  "author": "Bo",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/node": "^12.7.1",
    "@types/sqlite3": "^3.1.5",
    "artillery": "^1.6.0-28",
    "eslint": "^6.1.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "pre-push": "^0.1.1",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "winston": "^3.2.1"
  }
}